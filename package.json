  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "start": "next start",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!**/coverage/**",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "grid-styled": "^2.0.0-4",
    "next": "beta",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "styled-components": "^2.1.0",
    "styled-system": "^1.0.0-4"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.1.5",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1"
  }
}
